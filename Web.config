        </rule>
        <rule name="rule 11m" stopProcessing="true">
          <match url="^(.*)/(.*)-([0-9]+)\.html$" ignoreCase="true" />
          <action type="Rewrite" url="/index.php?page={R:1}&amp;katurl={R:2}&amp;id={R:3}&amp;lang=tr" />
        </rule>
        <rule name="rule 12m" stopProcessing="true">
          <match url="^(.*)/(.*)-([0-9]+)\.html$" ignoreCase="true" />
          <action type="Rewrite" url="/index.php?page={R:1}&amp;katurl={R:2}&amp;id={R:3}&amp;lang=tr" />
        </rule>
        <rule name="rule 13m" stopProcessing="true">
          <match url="^(.*)/(.*)\.html$" />
