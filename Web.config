<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="rule 18c" stopProcessing="true">
          <match url="^adminpanel/(.*)$" />
          <action type="Rewrite" url="/adminpanel/{R:1}" />
        </rule>
        <rule name="rule 1m" stopProcessing="true">
          <match url="^ajax/(.*)/(.*)\.html$" ignoreCase="true" />
          <action type="Rewrite" url="/index.php?page={R:1}&amp;type=ajax&amp;katurl={R:2}" />
        </rule>
        <rule name="rule 2m" stopProcessing="true">
          <match url="^ajax/(.*)\.html$" ignoreCase="true" />
          <action type="Rewrite" url="/index.php?page={R:1}&amp;type=ajax" />
        </rule>
        <rule name="rule 3m" stopProcessing="true">
          <match url="^en/(.*)/(.*)-([0-9]+)\.html$" ignoreCase="true" />
          <action type="Rewrite" url="/index.php?page={R:1}&amp;katurl={R:2}&amp;id={R:3}&amp;lang=en" />
        </rule>
        <rule name="rule 4m" stopProcessing="true">
          <match url="^en/(.*).html$" ignoreCase="true" />
          <action type="Rewrite" url="/index.php?page={R:1}&amp;lang=en" />
        </rule>
        <rule name="rule 5m" stopProcessing="true">
