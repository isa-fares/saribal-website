        </rule>
        <rule name="rule 18m" stopProcessing="true">
          <match url="^upload/(.*)$" />
          <action type="Rewrite" url="/upload/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
  <system.web>
